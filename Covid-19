package my_selenium_try;


import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Set;
import java.util.concurrent.TimeUnit;

import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.interactions.Actions;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;

public class Try_out_first {
	WebDriver driver;
  @Test
  public void f() {
	 driver.findElement(By.xpath("//input[@class='gLFyf gsfi']")).sendKeys("COVID-19");
	 driver.findElement(By.xpath("//input[@class='gLFyf gsfi']")).sendKeys(Keys.ENTER);
	 
	 driver.findElement(By.xpath("//a[@data-hveid='CAEQAw']")).click();
	 
	 ArrayList<String> mylist= new ArrayList<String>();
	 List<WebElement> mysearch=driver.findElements(By.xpath("//div[@class='XTjFC WF4CUc']"));
	 for(WebElement news: mysearch) {
		 mylist.add(news.getText());
	 }
	 System.out.println(mylist);
		 
	 Set<String> myunique= new HashSet<String>(mylist);
	 if(myunique.size()<3) {
		 System.out.println("Missing Leading News Agencies");
	 }
	 else {
		 System.out.println("Total no of unique news agency: "+myunique.size());
		 System.out.println(myunique);
	 }
  }
  @BeforeTest
  public void setup() {
	  
	  System.setProperty("webdriver.chrome.driver", "C:\\Users\\arun\\Favorites\\Downloads\\chromedriver_win32\\chromedriver.exe");
	  String url="https://www.google.com/";
	  driver= new ChromeDriver();
	  driver.get(url);
	  driver.manage().window().maximize();
	  System.out.println(driver.getTitle());
  }
}
